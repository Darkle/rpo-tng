{
  "name": "rpo-tng",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "snowpack --dest src/web_modules",
    "a": "node_modules/replace-html-script/bin/replace-html-script -s src/ts/index.js -i src/index.html -o src/index2.html",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:browsersync": "cd src && browser-sync --server --port 8888 --watch --no-open --no-ghost-mode --no-notify --no-ui --https",
    "dev:babel": "NODE_ENV=development babel -x .ts,.tsx src/ts --out-dir src/ts --ignore \"src/web_modules/**/*.*\" --source-maps --watch",
    "dev:typescript": "tsc --watch",
    "build": "CACHE_BUST_STRING=$(random string) NODE_ENV=production run-s build:**",
    "build:clean": "trash dist",
    "build:copy-to-dist": "ncp src dist",
    "build:snowpack": "NODE_ENV=production snowpack --optimize --no-source-map --stat --dest dist/web_modules",
    "build:babel": "babel -x .ts,.tsx src/ts --out-dir dist/js --ignore \"src/web_modules/**/*.*\"",
    "build:rename-js-files": "laren \"./dist/js/**/*.js\" \"f => f.replace('.js', '_' + process.env.CACHE_BUST_STRING + '.js')\"",
    "build:browsersync": "cd dist && browser-sync start --server --port 8888 --no-open --no-ghost-mode --no-notify --no-ui --https"
  },
  "browserslist": "last 3 Chrome version, Firefox ESR, last 1 safari version, last 3 ios version",
  "snowpack": {
    "webDependencies": [
      "lit-html"
    ]
  },
  "dependencies": {
    "lit-html": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "chalk-cli": "^4.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-lit": "^1.2.0",
    "execa": "^4.0.1",
    "fkill-cli": "^6.0.1",
    "laren": "^1.2.0",
    "lighthouse": "^5.6.0",
    "lit-analyzer": "^1.1.10",
    "ncat": "^4.0.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "orn": "^2.1.0",
    "pad-rename": "^1.1.0",
    "random-generator-cli": "^2.4.1",
    "recursive-rename": "^3.0.0",
    "replace-html-script": "^1.1.1",
    "replace-script-src": "git+https://gitlab.com/Darkle1/inline-img.git",
    "snowpack": "^1.7.1",
    "trash-cli": "^3.0.0",
    "ts-lit-plugin": "^1.1.10",
    "ts-rename-import-plugin": "^1.1.2",
    "ts-typie": "^1.1.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkle/rpo-tng.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
